        android:id="@+id/star3"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_marginStart="2dp"
        app:srcCompat="@drawable/ic_htl_star_rating"/>
    <ImageView
        android:id="@+id/star4"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_marginStart="2dp"
        app:srcCompat="@drawable/ic_htl_star_rating"/>
    <ImageView
        android:id="@+id/star5"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_marginStart="2dp"
        app:srcCompat="@drawable/ic_htl_star_rating"/>
</LinearLayout>
