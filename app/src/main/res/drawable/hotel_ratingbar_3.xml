<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:gravity="left|right|center_vertical|center_horizontal|fill_horizontal|center" android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="100dp"/>
            <solid android:color="@color/rating_bg_empty"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="100dp"/>
                <gradient android:startColor="@color/rating_bg_3" android:endColor="@color/rating_bg_3" android:angle="90"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="100dp"/>
                <gradient android:startColor="@color/rating_bg_3" android:endColor="@color/rating_bg_3" android:angle="90"/>
            </shape>
        </clip>
    </item>
</layer-list>
